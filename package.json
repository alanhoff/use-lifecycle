{
  "name": "use-lifecycle",
  "version": "1.0.0",
  "description": "A collection of hooks that helps you to track the lifecycle of your components in a similar way to class components.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "rollup -c",
    "test": "npm run build && eslint src && jest",
    "prepublishOnly": "npm run test"
  },
  "homepage": "https://github.com/alanhoff/use-lifecycle",
  "repository": {
    "type": "git",
    "url": "https://github.com/alanhoff/use-lifecycle.git"
  },
  "keywords": [
    "hook",
    "react",
    "custom",
    "lifecycle",
    "component",
    "class",
    "methods",
    "componentDidUpdate",
    "componentDidMount",
    "componentWillUnmount",
    "useDidUpdate",
    "useDidMount",
    "useWillUnmount"
